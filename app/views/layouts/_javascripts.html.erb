<!-- check to see if this controller/action allow tinymce before we load the gigantor js; see application_helper -->
<% if allow_tinymce?(controller) %>
  <%= yield :tinymce %>
<% end %>
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js" type="text/javascript"></script><!--updated-->
<script src="http://code.jquery.com/ui/1.10.0/jquery-ui.js" type="text/javascript"></script><!--updated, but change to google api min before deploy-->
<!-- if user has googleapis blocked for some reason we need a fallback -->
<script type="text/javascript">
if (typeof jQuery == 'undefined')
{
    document.write(unescape("%3Cscript src='/javascripts/jquery.min.js' type='text/javascript'%3E%3C/script%3E")); //updated
    document.write(unescape("%3Cscript src='/javascripts/jquery-ui.min.js' type='text/javascript'%3E%3C/script%3E")); //updated
}
</script>

<script type="text/javascript">$j = jQuery.noConflict();</script>
<%= javascript_include_tag 'jquery.livequery.min.js' %><!--no update available or needed-->
<%= javascript_include_tag 'rails' %><!--updated-->
<%= javascript_include_tag 'application' %>
<%= javascript_include_tag 'livevalidation_standalone' %>
<%= javascript_include_tag 'jquery.qtip.min.js' %><!--updated but needs jquery migrate-->
<%= javascript_include_tag 'jquery-shuffle.js' %><!--no update needed-->
<%= javascript_include_tag 'jquery.scrollTo.min.js' %><!--updated-->
<%= javascript_include_tag 'jquery.tokeninput.js' %><!--no update needed-->
<%= javascript_include_tag "ibox/ibox" %><!--no update needed-->
<%= javascript_include_tag "jquery.cookie" %>

<% if allow_tinymce?(controller) %>
  <%= yield :tinymce_init %>
<% end %>

<%= yield :footer_js %>