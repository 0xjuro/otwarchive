<% # load and cache site skin %>
<% set_site_skin %>
<% if @site_skin %>
  <% skin_cache_key, roles = get_skin_cache(@site_skin) %>
  <% cache(skin_cache_key) do %>
    <%= @site_skin.get_style(roles) %>
  <% end %>
<% end %>

